{
  "name": "port-me",
  "version": "0.1.1",
  "description": "Get an available port",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel ./src -d ./lib",
    "coverage": "nyc --reporter=html --reporter=text --reporter=text-lcov --reporter=lcov npm test | codacy-coverage --token=$CODACY_PROJECT_TOKEN",
    "prepublishOnly": "npm run tc && npm run build",
    "pretest": "standard | snazzy && eslint ./src",
    "report": "nyc npm test",
    "tc": "npm run test-with-coverage",
    "test": "NODE_ENV=test mocha ./test/",
    "test-with-coverage": "npm run report && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gavinvangent/port-me.git"
  },
  "keywords": [
    "port",
    "random",
    "get",
    "any"
  ],
  "author": "Gavin van Gent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gavinvangent/port-me/issues"
  },
  "homepage": "https://github.com/gavinvangent/port-me#readme",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.0.0",
    "random-int": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "assert": "1.4.1",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-istanbul": "5.1.3",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "codacy-coverage": "3.4.0",
    "eslint": "4.19.1",
    "mocha": "5.2.0",
    "nyc": "14.0.0",
    "pre-commit": "1.2.2",
    "snazzy": "7.1.1",
    "standard": "11.0.1"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": true,
    "include": [
      "src/**/*.js"
    ],
    "cache": true,
    "watermarks": {
      "lines": [
        95,
        100
      ],
      "functions": [
        95,
        100
      ],
      "branches": [
        95,
        100
      ],
      "statements": [
        95,
        100
      ]
    }
  },
  "pre-commit": [
    "build",
    "report"
  ],
  "standard": {
    "env": [
      "mocha"
    ],
    "parser": "babel-eslint"
  }
}
